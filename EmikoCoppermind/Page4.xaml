<Page x:Class="EmikoCoppermind.Page4"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EmikoCoppermind"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page4">

    <Grid Background="White">
        <Button Name="OpenExcelButton" Content="Open Excel File" Width="200" Height="50" Click="OpenExcelButton_Click" Background="#FFFFEB14" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,10,0,0"/>
        <DataGrid Name="ExcelDataGrid" AutoGenerateColumns="True" Margin="10,70,10,10"/>
    </Grid>
</Page>
